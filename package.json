{
  "name": "vue-base",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "concurrently npm:client npm:http npm:proxy",
    "client": "cross-env VUE_APP_ENV=dev vue-cli-service serve",
    "http": "npx nodemon --watch server/index.js --exec node server/index.js",
    "build": "cross-env VUE_APP_ENV=prod vue-cli-service build --report",
    "proxy": "npx http-server - -p 8079 -c-1 --cors --proxy http://127.0.0.1:8081",
    "build:test": "cross-env VUE_APP_ENV=test vue-cli-service build",
    "lint": "vue-cli-service lint"
  },
  "dependencies": {
    "@jledentu/vue-finder": "^2.0.1",
    "axios": "^0.21.1",
    "babel-plugin-component": "^1.1.1",
    "core-js": "^3.10.1",
    "element-ui": "^2.13.2",
    "express": "^4.18.2",
    "redaxios": "^0.5.1",
    "vue": "^2.6.12",
    "vue-router": "^3.5.1",
    "vuex": "^3.6.2"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "^4.5.12",
    "@vue/cli-plugin-eslint": "^4.5.12",
    "@vue/cli-plugin-router": "^4.5.12",
    "@vue/cli-plugin-vuex": "^4.5.12",
    "@vue/cli-service": "^4.5.12",
    "@vue/eslint-config-prettier": "^6.0.0",
    "babel-eslint": "^10.1.0",
    "concurrently": "^7.6.0",
    "cross-env": "^7.0.3",
    "eslint": "^6.8.0",
    "eslint-plugin-prettier": "^3.4.0",
    "eslint-plugin-vue": "^6.2.2",
    "http-server": "^14.1.1",
    "less": "^3.13.1",
    "less-loader": "^5.0.0",
    "nodemon": "^2.0.21",
    "prettier": "^2.2.1",
    "style-resources-loader": "^1.4.1",
    "vue-cli-plugin-style-resources-loader": "^0.1.5",
    "vue-template-compiler": "^2.6.12"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/essential",
      "eslint:recommended",
      "@vue/prettier"
    ],
    "parserOptions": {
      "parser": "babel-eslint"
    },
    "rules": {
      "prettier/prettier": [
        "error",
        {
          "singleQuote": true,
          "semi": false
        }
      ],
      "semi": [
        "error",
        "never"
      ],
      "quotes": [
        "error",
        "backtick"
      ],
      "no-unused-vars": "off",
      "no-console": "off",
      "no-debugger": "off"
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not dead"
  ]
}
